<!--pages/home/home.wxml-->

<!--所有的通知都没有的时候隐藏代办事项标题-->
<view wx:if="{{isfeed}}" class="cu-bar bg-white margin-top">
  <view class="action">
    <text class="cuIcon-titles text-green"></text>
    <text>待办事项</text>
  </view>
</view>

<view wx:if="{{isfeed}}" bindtap='overClick' class="grid col-1 padding-sm">
  <view class="padding-sm">
    <view class="bg-gradual-red padding radius text-center shadow-blur">
      <view class="text-lg" >喂食中（点击结束）</view>
      <view class="margin-top-sm text-Abc">{{Dietnote.begintime}}</view>
    </view>
  </view>
</view>
<view class="cu-bar bg-white margin-top">
  <view class="action">
    <text class="cuIcon-titles text-green"></text>
    <text>功能按钮</text>
  </view>
</view>
<view class="grid col-2 padding-sm">
  <view class="padding-sm" wx:for="{{ MenuList }}" wx:key>
    <view data-url="{{ item.url }}" class="bg-{{item.name}} padding radius text-center shadow-blur" bindtap='menuNavigateTo'>
      <view class="text-lg">{{item.title}}</view>
      <view class="margin-top-sm text-Abc">{{item.text}}</view>
    </view>
  </view>
</view>
